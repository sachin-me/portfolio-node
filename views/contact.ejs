  <link rel="stylesheet" href="css/style.css">
</head>

<body>

  <!-- Container -->
  <section class="main-container">
    <!-- Navbar -->
    <div class="navbar">
      <a href="" class="logo">
        <h1 class="logo__heading">S</h1>
      </a>

      <!-- Nav -->
      <nav class="nav">
        <a class="nav__link" href="index">
          <i class="home-icon fas fa-home"></i><span class="home">Home</span>
        </a>
        <a class="nav__link" href="about">
          <i class="about-icon fas fa-user"></i><span class="about">About</span>
        </a>
        <a class="nav__link" href="skill">
          <i class="skill-icon fas fa-cogs"></i><span class="skill">Skills</span>
        </a>
        <a class="nav__link" href="project">
          <i class="work-icon fas fa-eye"></i><span class="work">My Work</span>
        </a>
        <a class="nav__link" href="contact">
          <i class="contact-icon fas fa-envelope"></i><span class="contact">Contact</span>
        </a>
      </nav>

      <!-- Social -->
      <ul class="social">
        <li class="social__list">
          <a href="https://twitter.com/_im_sk" class="icon__link" target="_blank"><i class="fab fa-twitter"></i></a>
        </li>
        <li class="social__list">
          <a href="https://www.linkedin.com/in/sachin-kumar-tiwari-727aa2159/" class="icon__link" target="_blank"><i
              class="fab fa-linkedin-in"></i></a>
        </li>
        <li class="social__list">
          <a href="https://github.com/sachin-me" class="icon__link" target="_blank"><i class="fab fa-github"></i></a>
        </li>
        <li class="social__list">
          <a href="https://medium.com/@sachinkumartiwari363" class="icon__link" target="_blank"><i class="fab fa-medium-m"></i></a>
        </li>
      </ul>
    </div>

    <!-- Main-Page -->
    <div class="main-page">
      <div class="main">
        <div>
          <code>&lt;body&gt;</code>
        </div>
        <div class="main__details">
          <code>&lt;h1&gt;</code>
          <div class="form__container">
            <form action="/data" method="POST" class="form">
              <label for="firstName">
                <input type="text" id="firstName" name="firstName" placeholder="Enter your first name..">
              </label>
              <label for="lastName">
                <input type="text" id="lastName" name="lastName" placeholder="Enter your last name..">
              </label>
              <label for="email">
                <input type="email" id="email" name="email" placeholder="Enter your email..">
              </label>
              <label for="contactNumber">
                <input type="number" id="contactNumber" name="contactNumber" placeholder="Enter your contact number..">
              </label>
              <label for="message">
                <textarea name="textarea" id="message" placeholder="Enter your message here.."></textarea>
              </label>
              <div class="sbmit_btn_wrapper">
                <button class="submit__btn">Submit</button>
              </div>
            </form>
          </div>
          <code>&lt;/h1&gt;</code>
        </div>
        <div class="main__code">
          <code>&nbsp;&nbsp;&nbsp;&lt;/body&gt;</code>
          <code>&lt;/html&gt;</code>
        </div>

      </div>
      <!-- <div class="main-logo">
